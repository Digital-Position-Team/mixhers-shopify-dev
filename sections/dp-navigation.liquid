  </div>
</div>

{% schema %}
{
  "name": "DP Navigation",
  "settings": [
    {
      "type": "color",
      "id": "logo_color",
      "label": "Logo Color",
      "default": "#FF59E4"
    },
    {
      "type": "color",
      "id": "link_color",
      "label": "Link Color",
      "default": "#000000"
    },
    {
      "type": "color",
      "id": "icon_color",
      "label": "Icon Color",
      "default": "#000000"
    },
    {
      "type": "url",
      "id": "shop_link",
      "label": "Shop Link",
      "default": "/collections/all"
    },
    {
      "type": "text",
      "id": "shop_text",
      "label": "Shop Text",
      "default": "Shop"
    },
    {
      "type": "url",
      "id": "quiz_link",
      "label": "Quiz Link",
      "default": "/pages/quiz"
    },
    {
      "type": "text",
      "id": "quiz_text",
      "label": "Quiz Text",
      "default": "Take the Quiz"
    }
  ],
  "presets": [
    {
      "name": "DP Navigation"
    }
  ]
}
{% endschema %}
