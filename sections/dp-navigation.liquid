{% liquid
  assign logo_color = section.settings.logo_color
  assign icon_color = section.settings.icon_color
%}

<style>
  .dp-nav {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 20px 40px;
    font-family: 'Buenos Aires', sans-serif;
  }
  .dp-nav__logo a {
    display: block;
  }
  .dp-nav__links {
    display: flex;
    align-items: center;
    gap: 30px;
  }
  .dp-nav__links a {
    text-decoration: none;
    color: {{ section.settings.link_color }};
  }
  .dp-nav__icons {
    display: flex;
    align-items: center;
    gap: 20px;
  }
  .dp-nav__icons a {
    display: block;
  }
</style>

<div class="dp-nav">
